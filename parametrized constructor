#include<iostream>
using namespace std;
class construct{
    int a,b;
    public:
    construct(int m,int n)
    {
        a=m;
        b=n;
        
        
    }
    void display(){
        cout<<"a="<<a<<endl<<"b="<<b;
    }
};
int main(){
   construct c(23,45);
   c.display();
   return 0;
}
/*OUTPUT:
a=23
b=45

=== Code Execution Successful ===*/
