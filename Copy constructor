#include<iostream>
using namespace std;
class Student{
    int age;
    string name;
    public:
    Student(string n,int a){
        name=n;
        age=a;
    }
    Student(const Student &s){
        name=s.name;
        age=s.age;
        
    }
    void display(){
        cout<<"Name:"<<name<<endl<<"Age:"<<age<<endl;
    }
};
int main(){
  Student s1("Suyashi",19);
  Student s2=s1;
  cout<<"Original object:"<<endl;
  s1.display();
  cout<<"Copied object:"<<endl;
  s2.display();
  return 0;
}
/*OUTPUT:
Original object:
Name:Suyashi
Age:19
Copied object:
Name:Suyashi
Age:19


=== Code Execution Successful ===*/
